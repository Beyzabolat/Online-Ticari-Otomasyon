@using mvcOnlineTicariOtomasyon.Models.Classes
@model List<KargoTakip>
@{
    ViewBag.Title = "KargoTakip";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1 class="h3 mb-0 text-gray-800">Kargo Takip Sayfası</h1>
<br />
<table id="tbl6" class="table table-striped">
    <thead>
        <tr>
            <th>Açıklama</th>
            <th>Tarih</th>
            

        </tr>

    </thead>

    @foreach (var b in Model)
    {
        <tr>
            <td>@b.Aciklama</td>
            <td>@b.tarihzaman</td>
            

        </tr>
    }
</table>

